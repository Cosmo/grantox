<%= form_for @section do |f| %>
  <div class="row">
    <div class="col-md-9 col-sm-8">
      
      <div class="panel panel-default">
        <div class="panel-body">
          
          <%= render partial: "form", locals: { f: f } %>
          
        </div>
      </div>
      
    </div>
    <div class="col-md-3 col-sm-4">
      <div class="panel panel-default">
        <div class="panel-body">
          <%= f.submit "Create", class: "btn btn-primary" %>
        </div>
      </div>
    </div>
  </div>
<% end %>
